<div class="container">

 <div id="content">
    <ul id="tabs" class="nav nav-tabs" data-tabs="myTabs">
        <li class="active"><a href="#about" data-toggle="tab">About Me</a></li>
        <li><a href="#port" data-toggle="tab">Portfolio</a></li>
        <li><a href="#sandbox" data-toggle="tab">Sandbox!</a></li>
        <li><a href="#contact" data-toggle="tab">Contact Me</a></li>
    </ul>
    <div id="my-tab-content" class="tab-content">
        <div class="tab-pane active" id="about">
            <h1>About Me</h1>
            <%= render 'about.html.erb' %>
        </div>
        <div class="tab-pane" id="port">
            <h1>Portfolio</h1>
            <%= render 'portfolio.html.erb' %>
        </div>
        <div class="tab-pane" id="sandbox">
            <h1>Sandbox</h1>
            <%= render 'sandbox' %>
        </div>
        <div class="tab-pane" id="contact">
            <h1>Contact Me</h1>
            <%= render 'contact.html.erb' %>
        </div>
    </div>
 </div>


 <script type="text/javascript">
    jQuery(document).ready(function ($) {
        $('#myTabs').tab();
    });
 </script>    
</div>
